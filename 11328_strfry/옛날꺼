#include<string.h>
#include<stdio.h>

int main() {
	int N, i;
	char one[1001] = { '\0' }, two[1001] = { '\0' };
	int NO = 0;
	scanf("%d", &N);
	if (N > 1000)return 0;
	while (N > 0) {
		scanf("%s %s", &one, &two);
		int one_1[26] = { '0' }, two_1[26] = { '0' };
		for (i = 0; i < strlen(one); i++) {
			one_1[one[i] - 'a']++;
			two_1[two[i] - 'a']++;
		}
		for (i = 0; i < 26; i++) {
			if (one_1[i] != two_1[i]) {
				printf("Impossible\n");
				NO = 1;
				break;
			}
		}
		if (NO != 1) {
			printf("Possible\n");
		}

		N--;
		NO = 0;
	}
	return 0;
}
